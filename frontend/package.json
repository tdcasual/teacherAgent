{
  "name": "physics-agent-frontend",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "dev:student": "vite --config vite.student.config.ts",
    "dev:teacher": "vite --config vite.teacher.config.ts",
    "build:student": "vite build --config vite.student.config.ts",
    "build:teacher": "vite build --config vite.teacher.config.ts",
    "typecheck": "tsc -p tsconfig.json --noEmit",
    "e2e:teacher": "playwright test -c playwright.teacher.config.ts",
    "e2e:student": "playwright test -c playwright.student.config.ts",
    "e2e:mobile-menu": "npm run e2e:teacher -- e2e/teacher-session-sidebar.spec.ts --grep \"mobile session menu supports keyboard navigation and escape focus return\" && npm run e2e:student -- e2e/student-session-sidebar.spec.ts --grep \"mobile session menu supports keyboard navigation and escape focus return\"",
    "preview:student": "vite preview --config vite.student.config.ts",
    "preview:teacher": "vite preview --config vite.teacher.config.ts"
  },
  "dependencies": {
    "@tailwindcss/typography": "^0.5.19",
    "@tailwindcss/vite": "^4.1.18",
    "katex": "^0.16.11",
    "react": "^19.2.4",
    "react-dom": "^19.2.4",
    "react-resizable-panels": "^4.6.2",
    "rehype-katex": "^7.0.1",
    "rehype-sanitize": "^6.0.0",
    "rehype-stringify": "^10.0.0",
    "remark-gfm": "^4.0.0",
    "remark-math": "^6.0.0",
    "remark-parse": "^11.0.0",
    "remark-rehype": "^11.1.0",
    "tailwindcss": "^4.1.18",
    "unified": "^11.0.4",
    "unist-util-visit": "^5.0.0"
  },
  "devDependencies": {
    "@playwright/test": "^1.58.2",
    "@types/react": "^19.2.13",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^5.1.4",
    "typescript": "^5.9.3",
    "vite": "^7.3.1",
    "vite-plugin-pwa": "^1.2.0"
  }
}
